<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="myorg" class="com.adithya.springdemo.domain.Organization"
		c:companyName="Porsche" c:yearOfIncorporation="1994" c:address-ref="Address" />

	<bean id="Address" class="com.adithya.springdemo.domain.Address"
		c:city="Kozhikode" c:street="Thiruvannur" c:zipCode="984747"></bean>
</beans>
